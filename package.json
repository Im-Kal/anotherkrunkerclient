{
  "name": "anotherkrunkerclient",
  "productName": "anotherkrunkerclient",
  "version": "0.1.0",
  "description": "Another damn Krunker client",
  "author": "Asger Finding",
  "license": "GPLV3",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asger-finding/anotherkrunkerclient.git"
  },
  "scripts": {
    "start": "yarn compile && electron ./build/main.js",
    "compile": "tsc && copyfiles -u 1 -e src/**/*.ts -e src/**/*.ts src/**/* build/",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "lint": "yarn eslint . --ext .ts,.js",
    "lintcss": "yarn stylelint src/**/*.css"
  },
  "_moduleAliases": {
    "@constants": "./src/constants.ts"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "electron": "^9.4.4",
    "electron-acrylic-window": "^0.5.11",
    "electron-log": "^4.4.6",
    "electron-prompt": "^1.7.0",
    "electron-store": "^8.0.1",
    "electron-updater": "4.6.5",
    "module-alias": "^2.2.2",
    "semver": "^7.3.5",
    "v8-compile-cache": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "12.12.21",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "copyfiles": "^2.4.1",
    "electron-builder": "^22.14.13",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-typescript": "^16.1.1",
    "eslint-config-raven": "^1.0.3",
    "eslint-plugin-import": "^2.25.4",
    "stylelint": "^14.5.3",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.6.2"
  }
}
