{
  "name": "anotherkrunkerclient",
  "productName": "anotherkrunkerclient",
  "version": "0.1.0",
  "description": "Another damn Krunker client",
  "author": "Asger Finding",
  "license": "GPLV3",
  "private": true,
  "main": "build/main.js",
  "repository": "asger-finding/anotherkrunkerclient",
  "scripts": {
    "start": "yarn del ./build && yarn build && yarn copy && electron ./build/main.js",
    "deploy": "yarn del ./build ./dist && yarn build && yarn copy && electron-builder --win && yarn postinstall",
    "build": "tsc",
    "copy": "copyfiles -u 1 -e ./src/**/*.ts ./src/**/* ./build/",
    "postinstall": "electron-builder install-app-deps && yarn del ./build",
    "lint": "yarn eslint . --ext .ts,.js",
    "lintcss": "yarn stylelint src/**/*.css"
  },
  "build": {
    "appId": "com.electron.akc",
    "copyright": "Copyright Â© 2022 Asger Finding",
    "publish": "github",
    "asar": true,
    "compression": "maximum",
    "files": [
      "build/**/*",
      "package.json"
    ],
    "directories": {
      "output": "dist"
    },
    "forceCodeSigning": false,
    "artifactName": "anotherkrunkerclient-${os}-${arch}.${ext}",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "media/icon.ico"
    },
    "nsis": {
      "artifactName": "anotherkrunkerclient-Windows-${arch}.exe",
      "installerSidebar": "media/installerSidebar.bmp",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true
    }
  },
  "dependencies": {
    "axios": "^0.26.1",
    "electron-acrylic-window": "^0.5.11",
    "electron-is-dev": "^2.0.0",
    "electron-localshortcut": "^3.2.1",
    "electron-log": "^4.4.6",
    "electron-prompt": "^1.7.0",
    "electron-store": "^8.0.1",
    "electron-updater": "4.6.5",
    "module-alias": "^2.2.2",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@types/node": "12.12.21",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "copyfiles": "^2.4.1",
    "del-cli": "^4.0.1",
    "electron": "^9.4.3",
    "electron-builder": "^22.14.13",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-typescript": "^16.1.1",
    "eslint-config-raven": "^1.0.3",
    "eslint-plugin-import": "^2.25.4",
    "stylelint": "^14.5.3",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-standard": "^25.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  }
}
