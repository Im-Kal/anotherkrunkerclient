{
	"extends": "raven",
	"parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
	"rules": {
		"linebreak-style": "off",
		"max-classes-per-file": "off",
		"sort-keys": "off",
		"array-bracket-spacing": [ "error", "always", { "singleValue": false } ]
	},
	"env": {
		"browser": true,
		"node": true
	}
}
